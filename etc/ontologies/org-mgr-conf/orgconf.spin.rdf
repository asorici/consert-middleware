  <spl:ConstraintTemplate rdf:ID="ExclusiveExistencePropertyPairConstraint">
    <rdfs:subClassOf rdf:resource="http://spinrdf.org/spl#PropertyPairConstraintTemplates"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ExclusiveExistencePropertyPairConstraint</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >SPIN Constraint specifying that a class must have values for either one property or the other, but not both and not none.</rdfs:comment>
    <spin:body>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <sp:Filter>
            <sp:expression>
              <sp:or>
                <sp:arg1>
                  <sp:exists>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                        <sp:predicate rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >property</sp:varName>
                        </sp:predicate>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >someValue</sp:varName>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                        <sp:predicate rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >otherProperty</sp:varName>
                        </sp:predicate>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >someOtherValue</sp:varName>
                        </sp:object>
                      </rdf:Description>
                    </sp:elements>
                  </sp:exists>
                </sp:arg1>
                <sp:arg2>
                  <sp:notExists>
                    <sp:elements rdf:parseType="Collection">
                      <rdf:Description>
                        <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                        <sp:predicate rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >property</sp:varName>
                        </sp:predicate>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >someValue</sp:varName>
                        </sp:object>
                      </rdf:Description>
                      <rdf:Description>
                        <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
                        <sp:predicate rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >otherProperty</sp:varName>
                        </sp:predicate>
                        <sp:object rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >someOtherValue</sp:varName>
                        </sp:object>
                      </rdf:Description>
                    </sp:elements>
                  </sp:notExists>
                </sp:arg2>
              </sp:or>
            </sp:expression>
          </sp:Filter>
          <sp:Bind>
            <sp:variable rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >message</sp:varName>
            </sp:variable>
            <sp:expression>
              <sp:concat>
                <sp:arg1>Either </sp:arg1>
                <sp:arg2 rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >property</sp:varName>
                </sp:arg2>
                <sp:arg3> or </sp:arg3>
                <sp:arg4 rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >otherProperty</sp:varName>
                </sp:arg4>
                <sp:arg5> must have a value, but not both.</sp:arg5>
              </sp:concat>
            </sp:expression>
          </sp:Bind>
        </sp:where>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:nodeID="A0"/>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object rdf:resource="http://spinrdf.org/spin#ConstraintViolation"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A0"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationRoot"/>
            <sp:object rdf:resource="http://spinrdf.org/spin#_this"/>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:nodeID="A0"/>
            <sp:predicate rdf:resource="http://spinrdf.org/spin#violationPath"/>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >property</sp:varName>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
