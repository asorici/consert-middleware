    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >nrSuccessfulDerivations</rdfs:label>
    <rdfs:domain rdf:resource="#AssertionSpecificInferenceStatistic"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="timeSinceLastQuery">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#AssertionSpecificQueryStatistic"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >timeSinceLastQuery</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Property stating the time (in milliseconds) elapsed since the last reception of a query for the given ContextAssertion.</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="setsSchedulingType">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#InferenceSchedulingCommand"/>
          <owl:Class rdf:about="#QuerySchedulingCommand"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#SchedulingType"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >setsSchedulingType</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
